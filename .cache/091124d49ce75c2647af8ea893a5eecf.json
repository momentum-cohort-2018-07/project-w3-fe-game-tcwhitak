) {\n  return !(\n    b1 === b2 ||\n          b1.center.x + b1.size.x / 2 < b2.center.x - b2.size.x / 2 ||\n          b1.center.y + b1.size.y / 2 < b2.center.y - b2.size.y / 2 ||\n          b1.center.x - b1.size.x / 2 > b2.center.x + b2.size.x / 2 ||\n          b1.center.y - b1.size.y / 2 > b2.center.y + b2.size.y / 2\n  )\n}\n"},"lineCount":null}},"hash":"061c0252723c296f085c93535300885e","cacheData":{"env":{}}}